{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2020",
    "moduleResolution": "bundler",
    "jsx": "react-jsx"
  },
  "extends": "expo/tsconfig.base"
}
